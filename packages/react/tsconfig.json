{
	"extends": "../../tsconfig.base.json",
	"compilerOptions": {
		"rootDir": "src",
		"outDir": "dist",
		"module": "CommonJS",
		"tsBuildInfoFile": "dist/.tsbuildinfo",
		"composite": true,
		"importsNotUsedAsValues": "remove",
		"declarationMap": true,
		"verbatimModuleSyntax": false,
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true
	},
	"include": ["src"]
}
